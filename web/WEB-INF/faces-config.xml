<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <navigation-rule>
        <from-view-id>/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>reg</from-outcome>
            <to-view-id>/reg.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>admin/admin</from-outcome>
            <to-view-id>/admin/admin.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>error</from-outcome>
            <to-view-id>/error.xhtml</to-view-id>
        </navigation-case>

    </navigation-rule>
    <navigation-rule>
        <from-view-id>/error.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>reg</from-outcome>
            <to-view-id>/reg.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>index</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/reg.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>index</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/admin/admin.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>admin/guestsadmin</from-outcome>
            <to-view-id>/admin/guestsadmin.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>index</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>admin/treatmentadmin</from-outcome>
            <to-view-id>/admin/treatmentadmin.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>admin/reload</from-outcome>
            <to-view-id>/admin/reload.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/admin/guestsadmin.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>admin/admin</from-outcome>
            <to-view-id>/admin/admin.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>delete</from-outcome>
            <to-view-id>/admin/delete.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>addguest</from-outcome>
            <to-view-id>/admin/addguest.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/admin/delete.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>admin/guestsadmin</from-outcome>
            <to-view-id>/admin/guestsadmin.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/admin/addguest.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>admin/guestsadmin</from-outcome>
            <to-view-id>/admin/guestsadmin.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/admin/treatmentadmin.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>admin/admin</from-outcome>
            <to-view-id>/admin/admin.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>deletetreatment</from-outcome>
            <to-view-id>/admin/deletetreatment.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>addtreatment</from-outcome>
            <to-view-id>/admin/addtreatment.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/admin/deletetreatment.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>treadmin</from-outcome>
            <to-view-id>/admin/treatmentadmin.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/admin/reload.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>admin/admin</from-outcome>
            <to-view-id>/admin/admin.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/admin/addtreatment.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>admin/treatmentadmin</from-outcome>
            <to-view-id>/admin/treatmentadmin.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>
